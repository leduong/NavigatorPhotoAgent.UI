{
  "compilerOptions": {
    "outDir": "wwwroot",
    "target": "es5",
    "module": "system",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "lib": [ "es5", "dom" ]
  },
  "exclude": [
    "gulpfile.ts",
